<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308" prefHeight="877.0" prefWidth="1751.0" stylesheets="@V3PMGUI.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.v3pm_prototype.view.controller.ScenarioComparisonController">
   <children>
      <Label styleClass="Titel1" text="Solution Comparison" />
      <Label styleClass="Titel2" text="Roadmap &amp; NPV:">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Label>
      <VBox>
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox prefHeight="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label fx:id="lblScenarioName1" styleClass="Scenario1" text="Scenario Name 1" textFill="#f3612d" />
                        <VBox fx:id="roadmapBox1" prefHeight="112.0" prefWidth="100.0" />
                        <Label fx:id="lblNPV1" text="NPV: 1234123" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label fx:id="lblScenarioName2" styleClass="Scenario2" text="Scenario Name 2" />
                        <VBox fx:id="roadmapBox2" prefHeight="200.0" prefWidth="100.0" />
                        <Label fx:id="lblNPV2" text="NPV: 1231231" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <Label fx:id="lblSolution" alignment="TOP_LEFT" maxWidth="1.7976931348623157E308" prefHeight="71.0" text="Sample TextTwo lines" wrapText="true" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets top="4.0" />
         </VBox.margin>
      </Label>
      <Label styleClass="Titel1" text="Process Comparison">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
      <Label styleClass="Titel2" text="Process Development Delta:">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Label>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <BarChart fx:id="bcQuality" animated="false" legendVisible="false" minHeight="220.0" prefHeight="300.0" title="Quality" HBox.hgrow="ALWAYS">
              <xAxis>
                <CategoryAxis label="Process" side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis label="Delta" side="LEFT" />
              </yAxis>
            </BarChart>
            <BarChart fx:id="bcTime" animated="false" legendVisible="false" minHeight="220.0" prefHeight="300.0" title="Time" HBox.hgrow="ALWAYS">
               <xAxis>
                  <CategoryAxis label="Process" side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis label="Delta" side="LEFT" />
               </yAxis>
            </BarChart>
            <BarChart fx:id="bcOOP" animated="false" legendVisible="false" minHeight="220.0" prefHeight="300.0" title="Operating Outflows" HBox.hgrow="ALWAYS">
               <xAxis>
                  <CategoryAxis label="Process" side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis label="Delta" side="LEFT" />
               </yAxis>
            </BarChart>
            <BarChart fx:id="bcFixedCosts" animated="false" legendVisible="false" minHeight="220.0" prefHeight="300.0" title="Fixed Costs" HBox.hgrow="ALWAYS">
               <xAxis>
                  <CategoryAxis label="Process" side="BOTTOM" />
               </xAxis>
               <yAxis>
                  <NumberAxis label="Delta" side="LEFT" />
               </yAxis>
            </BarChart>
         </children>
      </HBox>
      <HBox>
         <children>
            <VBox>
               <children>
                  <Label styleClass="Titel1" text="Restriction Comparison">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Label>
                  <Label styleClass="Titel2" text="Broken Restrictions per Scenario:">
                     <padding>
                        <Insets bottom="10.0" />
                     </padding>
                  </Label>
                  <BarChart fx:id="bcBrokenRestrictions" animated="false">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis label="#Broken Restrictions" side="LEFT" />
                    </yAxis>
                  </BarChart>
               </children>
            </VBox>
            <AreaChart fx:id="sacCashflows" HBox.hgrow="ALWAYS">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </AreaChart>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
